{
    "name": "nddcoder/rest-framework",
    "description": "Framework for reactphp",
    "keywords": ["framework", "reactphp"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dung Nguyen Dang",
            "email": "dangdungcntt@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "twig/twig": "^3.0",
        "react/http": "^1.1",
        "vlucas/phpdotenv": "^5.0",
        "nikic/fast-route": "^1.3",
        "christoph-kluge/reactphp-http-cors-middleware": "^2.0"
    },
    "autoload": {
        "files": [
            "src/Support/helpers.php"
        ],
        "psr-4": {
            "Rest\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
